{
  "name": "claimable",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "ts-node scripts/validate-env.ts",
    "build:claimable-data": "npm run fetch-claimable-data && npm run build-master-data",
    "fetch-claimable-data": "ts-node scripts/fetch-claimable-data.ts",
    "build-master-data": "ts-node scripts/build-claimable-account-master.ts"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/hapi__joi": "^17.1.6",
    "csv-parse": "^4.15.1",
    "dotenv": "^8.2.0",
    "ethers": "^5.0.29",
    "next": "10.0.4",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "react": "17.0.1",
    "react-confetti": "^6.0.0",
    "react-dom": "17.0.1",
    "react-ga": "^3.3.0",
    "react-github-corner": "^2.5.0",
    "resize-observer": "^1.0.0",
    "styled-components": "^5.2.1",
    "ts-node": "^9.1.1",
    "use-resize-observer": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.24",
    "@types/node-fetch": "^2.5.8",
    "@types/react": "^17.0.1",
    "@types/react-dom": "17.0.0",
    "@types/styled-components": "^5.1.7",
    "babel-plugin-styled-components": "^1.12.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build:claimable-data && npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,md,json,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2
  }
}
